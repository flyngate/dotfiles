
!!! urxvt begin

urxvt.perl-ext-common: selection-to-clipboard
urxvt.saveLines:       3200
urxvt.scrollBar:       false
!urxvt.cursorUnderline: 1
!urxvt.cursorBlink:     true

!! Color scheme

#include ".colors/invisibone"
!#include ".colors/dusty"
!#include ".colors/earth"
!#include ".colors/sero"
!#include ".colors/blush"
!#include ".colors/light"

!! Fonts

!urxvt.font:           -*-terminus-*-*-*-*-*-*-*-*-*-*-*-*
!urxvt.boldFont:       -*-terminus-*-*-*-*-*-*-*-*-*-*-*-*
!urxvt.italicFont:     -*-terminus-*-*-*-*-*-*-*-*-*-*-*-*

!urxvt.font:           xft:Droid Sans Mono:size=8
!urxvt.boldFont:       xft:Droid Sans Mono:size=8
!urxvt.italicFont:     xft:Droid Sans Mono:italic:size=8
!urxvt.letterSpace:    -1.5

!urxvt.font:           -*-uushi-*-*-*-*-*-*-*-*-*-*-*-*
!urxvt.boldFont:       -*-uushi-*-*-*-*-*-*-*-*-*-*-*-*
!urxvt.italicFont:     -*-uushi-*-*-*-*-*-*-*-*-*-*-*-*

!urxvt.font:           -*-lemon-*-*-*-*-*-*-*-*-*-*-*-*
!urxvt.boldFont:       -*-lemon-*-*-*-*-*-*-*-*-*-*-*-*
!urxvt.italicFont:     -*-lemon-*-*-*-*-*-*-*-*-*-*-*-*

!urxvt.font:           xft:Tewi:style=Regular
!urxvt.boldFont:       xft:Tewi:style=Regular
!urxvt.italicFont:     xft:Tewi:style=Regular

!urxvt.font:           -*-termsyn-medium-*-*-*-11-*-*-*-*-*-*-*
!urxvt.boldFont:       -*-termsyn-bold-*-*-*-11-*-*-*-*-*-*-*
!urxvt.italicFont:     -*-termsyn-medium-*-*-*-11-*-*-*-*-*-*-*
!urxvt.boldItalicFont: -*-termsyn-bold-*-*-*-11-*-*-*-*-*-*-*

urxvt.font:           xft:Monaco:style=Regular:size=8
urxvt.boldFont:       xft:Monaco:style=Regular:size=8
urxvt.italicFont:     xft:Monaco:style=Italic:size=8
urxvt.letterSpace:    -1.0

!!! urxvt end

Xft.dpi:       96
Xft.rgba:      rgb
Xft.antialias: 1
Xft.hinting:   1
Xft.autohint:  0
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault

Xcursor.theme: Vanilla-DMZ

